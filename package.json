{
  "name": "u-json-docs",
  "version": "0.0.8",
  "author": "uStudio Front-end Crew",
  "repository": {
    "type": "git",
    "url": "https://github.com/uStudioCompany/u-json-docs.git"
  },
  "scripts": {
    "dev": "parcel src/test/index.html --no-cache",
    "clean": "rimraf lib",
    "build:copy": "copyfiles README.md LICENSE package.json lib",
    "build:components": "babel src --out-dir lib --extensions .ts,.tsx",
    "build:dts": "tsc -p tsconfig.prod.json",
    "build": "npm-run-all clean -p build:components build:dts build:copy -s",
    "release": "standard-version",
    "publication": "yarn build && yarn publish lib"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.1",
    "husky": "^4.2.5",
    "json-schema": "^0.2.5",
    "lint-staged": "^10.2.9",
    "parcel": "^1.12.4",
    "regenerator-runtime": "^0.13.5",
    "ustudio-ui": "^1.3.0-beta.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-plugin-styled-components": "^1.10.7",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "styled-components": "^5.1.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "lint-staged": {
    ".": [
      "yarn lint",
      "yarn format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT"
}
